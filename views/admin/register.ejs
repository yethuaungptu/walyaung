<!DOCTYPE html>
<html lang="en">
  <head>
    <% include ../partialA/head %>
  </head>
  <body>
    <div class="wrapper">
      <div class="main-header">
        <nav class="navbar bg-primary">
          <div>
            <a href="/admin" class="logo">
              <img
                src="/images/wy.png"
                alt="navbar brand"
                class="navbar-brand"
              />
            </a>
          </div>
        </nav>
        <!-- End Navbar -->
      </div>

      <!-- Sidebar -->
      <!-- <% include ../partialA/sidebar %> -->
      <!-- End Sidebar -->

      <div class="main-panel">
        <div class="content">
          <div class="row mt-5">
            <div class="col-lg-2"></div>
            <div class="card card-round col-lg-6 col-md-6 col-sm-12">
              <div class="card-title mx-auto mt-3">Admin Registration</div>
              <div class="card-body">
                <form action="/admin/register" method="post">
                  <div class="form-group">
                    <label for="name">Name</label>
                    <input
                      type="text"
                      class="form-control"
                      id="name"
                      name="name"
                      placeholder="Enter Name"
                      required
                    />
                  </div>
                  <div class="form-group">
                    <label for="email">Email Address</label>
                    <input
                      type="email"
                      class="form-control"
                      id="email"
                      name="email"
                      placeholder="Enter Email"
                      required
                    />
                  </div>
                  <div class="form-group">
                    <label for="password">Password</label>
                    <input
                      type="password"
                      class="form-control"
                      id="password"
                      name="password"
                      placeholder="Enter Password"
                      required
                    />
                  </div>
                  <div class="form-group">
                    <label for="recode">Reg Code</label>
                    <input
                      type="text"
                      class="form-control"
                      id="recode"
                      name="recode"
                      placeholder="Enter Reg Code"
                      required
                    />
                  </div>
                  <div class="form-group">
                    <input
                      type="submit"
                      class="btn btn-primary btn-block"
                      value="Registration"
                    />
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--   Core JS Files   -->
    <script src="/adassets/js/core/jquery.3.2.1.min.js"></script>
    <script src="/adassets/js/core/popper.min.js"></script>
    <script src="/adassets/js/core/bootstrap.min.js"></script>

    <!-- jQuery UI -->
    <script src="/adassets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="/adassets/js/plugin/jquery-ui-touch-punch/jquery.ui.touch-punch.min.js"></script>

    <!-- jQuery Scrollbar -->
    <script src="/adassets/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>

    <!-- Chart JS -->
    <script src="/adassets/js/plugin/chart.js/chart.min.js"></script>

    <!-- jQuery Sparkline -->
    <script src="/adassets/js/plugin/jquery.sparkline/jquery.sparkline.min.js"></script>

    <!-- Chart Circle -->
    <script src="/adassets/js/plugin/chart-circle/circles.min.js"></script>

    <!-- Datatables -->
    <script src="/adassets/js/plugin/datatables/datatables.min.js"></script>

    <!-- Bootstrap Notify -->
    <script src="/adassets/js/plugin/bootstrap-notify/bootstrap-notify.min.js"></script>

    <!-- jQuery Vector Maps -->
    <script src="/adassets/js/plugin/jqvmap/jquery.vmap.min.js"></script>
    <script src="/adassets/js/plugin/jqvmap/maps/jquery.vmap.world.js"></script>

    <!-- Sweet Alert -->
    <script src="/adassets/js/plugin/sweetalert/sweetalert.min.js"></script>

    <!-- Atlantis JS -->
    <script src="/adassets/js/atlantis.min.js"></script>

    <!-- Atlantis DEMO methods, don't include it in your project! -->
    <!-- <script src="/adassets/js/setting-demo.js"></script> -->
    <!-- <script src="/adassets/js/demo.js"></script> -->
  </body>
</html>
